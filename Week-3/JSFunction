console.log("3 factorial equals " + myfactorial(3));

/the following line results in error: mycube is not a function */
//console.log("3 cubed equals " + mycube(3));

function myfactorial(x) {
	var result = 1;
	for (var i = 2; i <= x; i++) {
		result *= i;
	}
	return result;
}

var mycube = function(x) {
	return x * x * x;
}

/* Now line 4 runs successfully afterfunction definition */
console.log("3 cubed equals " + mycube(3));